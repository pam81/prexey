{% extends '::backend.html.twig' %}

{% block container -%}
    <h3>Nuevo Cliente</h3>
    
   
    <div class="well">
    <div id="myTabContent" class="tab-content">
    <div class="tab-pane active in" id="home">
    <form class="form-horizontal" id="tab" name="tab" method="post" novalidate="true"    action="{{ path('cliente_create') }}" >
     {{ form_errors(form) }}
   

    
    {% include 'BackendAdminBundle:Cliente:_cliente.html.twig' %} 

    
    {{ form_rest(form) }}

    <div class="control-group">
    <!-- Button -->
    <div class="controls">
    <button type="submit" class="btn btn-primary"><i class="icon-ok icon-white"></i>Crear</button>
    <button type="button" class="btn btn-inverse" data-url="{{ path('cliente') }}"><i class="icon-chevron-left icon-white"></i> Back</button>
    
    </div>
    
    
    
    </div>
    </form>
    </div>
    
     </div>
      </div>
    

       
{% endblock %}
{% block javascripts %}
  {{ parent() }}
 
{% javascripts '@BackendAdminBundle/Resources/public/js/validate_cliente.js' %}
    <script type="text/javascript" src="{{ asset_url }}"></script>
{% endjavascripts %}
{% endblock %}
