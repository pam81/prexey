{% extends '::backend.html.twig' %}

{% block container -%}

{% set active=3 %}
  {% include 'BackendAdminBundle:Viaje:menu_viaje.html.twig' %}
  
  
   <div class="well">  
    

    <h3>Nuevo Movimiento</h3>
    
   
    <div class="well">
    <div id="myTabContent" class="tab-content">
    <div class="tab-pane active in" id="home">
    <form class="form-horizontal" id="tabMov" name="tabMov" method="post" novalidate="true"    action="{{ path('movimiento_create') }}" >
     {{ form_errors(form) }}
   

    
    {% include 'BackendAdminBundle:Movimiento:_movimiento.html.twig' %} 

    
    {{ form_rest(form) }}

    <div class="control-group">
    <!-- Button -->
    <div class="controls">
    <button type="submit" class="btn btn-primary"><i class="icon-ok icon-white"></i>Crear</button>
    <button type="button" class="btn btn-inverse" data-url="{{ path('caja') }}"><i class="icon-chevron-left icon-white"></i> Back</button>
    
    </div>
    
    
    
    </div>
    </form>
    </div>
    </div>
    </div>
    </div>     
{% endblock %}
{% block javascripts %}
  {{ parent() }}
{% javascripts '@BackendAdminBundle/Resources/public/js/movimiento.js' %}
    <script type="text/javascript" src="{{ asset_url }}"></script>
{% endjavascripts %} 
 
{% javascripts '@BackendAdminBundle/Resources/public/js/validate_movimiento.js' %}
    <script type="text/javascript" src="{{ asset_url }}"></script>
{% endjavascripts %}
{% endblock %}
