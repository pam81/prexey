<div class="nav-collapse collapse">
                {% if not is_granted('ROLE_VISITOR') %}
                
                <ul class="nav">
                    {% if is_granted('ROLE_VIEWUSER') %}
                    <li><a href="{{ path('user') }}">Usuarios</a></li>
                    {% endif %}
                    
                    {% if  is_granted('ROLE_SETEO') or is_granted('ROLE_ADMIN') or
                    is_granted('ROLE_VIEWPROVINCIA') or is_granted('ROLE_VIEWZONA') 
                     %}
                    <li class="dropdown">           <a href="#" class="dropdown-toggle" data-toggle="dropdown">Configuración<b class="caret"></b></a>
                      <ul class="dropdown-menu">
                        {% if is_granted('ROLE_ADMIN') %}
                            <li><a href="{{ path('group') }}">Accesos</a></li>
                        {% endif %}
                        {% if is_granted('ROLE_SETEO') %}
                            <li class="divider"></li>
                            <li><a href="{{ path('seteo') }}">Seteos</a></li>
                        {% endif %}
                        {% if is_granted('ROLE_VIEWPROVINCIA') %}
                            <li class="divider"></li>
                            <li><a href="{{ path('provincia') }}">Provincias</a></li>
                        {% endif %}
                        {% if is_granted('ROLE_VIEWZONA') %}
                            <li class="divider"></li>
                            <li><a href="{{ path('zona') }}">Zonas</a></li>
                        {% endif %}
                      </ul>
                    </li>
                    {% endif %}
                    
                    {% if  is_granted('ROLE_VIEWCLIENTE') or is_granted('ROLE_VIEWSUCURSAL') or
                       is_granted('ROLE_VIEWMARCA') or  is_granted('ROLE_VIEWMODELO') or 
                       is_granted('ROLE_VIEWSINTOMA') or is_granted('ROLE_VIEWTIPOARTICULO')
                       or is_granted('ROLE_VIEWARTICULO')
                     %}
                    
                    <li class="dropdown">
                    <a href="#" class="dropdown-toggle" data-toggle="dropdown">Entidades<b class="caret"></b></a>
                      <ul class="dropdown-menu" >
                       {% if is_granted('ROLE_VIEWCLIENTE') %}
                       <li> <a href="{{ path('cliente') }}"> Clientes</a> </li>
                       <li class="divider"></li>
                       {% endif %}
                       {% if is_granted('ROLE_VIEWSUCURSAL') %}
                       <li> <a href="{{ path('sucursal') }}"> Sucursales</a> </li>
                       <li class="divider"></li>
                       {% endif %}
                       {% if is_granted('ROLE_VIEWMARCA') %}
                       <li> <a href="{{ path('marca') }}"> Marcas</a> </li>
                       <li class="divider"></li>
                       {% endif %}
                       {% if is_granted('ROLE_VIEWMODELO') %}
                       <li> <a href="{{ path('modelo') }}"> Modelos</a> </li>
                       <li class="divider"></li>
                       {% endif %}
                       {% if is_granted('ROLE_VIEWSINTOMA') %}
                       <li> <a href="{{ path('sintoma') }}"> Sintomas</a> </li>
                       <li class="divider"></li>
                       {% endif %}
                       {% if is_granted('ROLE_VIEWTIPOARTICULO') %}
                       <li> <a href="{{ path('tipo_articulo') }}"> Tipo Artículos</a> </li>
                       <li class="divider"></li>
                       {% endif %}
                       {% if is_granted('ROLE_VIEWARTICULO') %}
                       <li> <a href="{{ path('articulo') }}"> Artículos</a> </li>
                       <li class="divider"></li>
                       {% endif %}
                      </ul>
                      
                    </li> 
                      {% endif %}
                </ul>
              {% endif %}  
                
             {% if is_granted('IS_AUTHENTICATED_REMEMBERED') %}
                
                <ul class="nav pull-right">
                   
                    <li class="dropdown">
                        <a href="#" class="dropdown-toggle" data-toggle="dropdown">{{ app.user.username }}  <b class="caret"></b></a>
                        <ul class="dropdown-menu">
                            <li><a href="{{ path('profile') }}">Perfil</a></li>
                            
                            <li class="divider"></li>
                            <li><a href="{{ path('logout') }}">Salir</a></li>
                        </ul>
                    </li>
                </ul>
                {% endif %}    
              
             
            </div><!--/.nav-collapse -->
